{
  "name": "scraper-ql",
  "version": "1.0.1",
  "description": "Web scraper based on GraphQL",
  "main": "src/index.js",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node -r dotenv/config src/index.js",
    "dev": "nodemon --experimental-specifier-resolution=node -r dotenv/config src/index.js",
    "lint": "eslint ./src "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrijshikhar/scraper-ql.git"
  },
  "author": "Adrij Shikhar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adrijshikhar/scraper-ql/issues"
  },
  "homepage": "https://github.com/adrijshikhar/scraper-ql#readme",
  "engines": {
    "node": ">=15.x"
  },
  "keywords": [
    "ES",
    "MODULES",
    "NODE",
    "MODULES",
    "JS"
  ],
  "type": "module",
  "devDependencies": {
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^5.1.1",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "prettier-eslint": "^12.0.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.10",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "jquery": "^3.6.0",
    "jsdom": "^16.6.0",
    "node-fetch": "^2.6.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
