{
  "name": "scraper-ql",
  "version": "1.0.1",
  "description": "Web scraper based on GraphQL",
  "main": "src/index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc --build --verbose",
    "dev": "nodemon -r dotenv/config src/index.ts",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "prepare": "husky install"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrijshikhar/scraper-ql.git"
  },
  "author": "Adrij Shikhar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adrijshikhar/scraper-ql/issues"
  },
  "homepage": "https://github.com/adrijshikhar/scraper-ql#readme",
  "engines": {
    "node": ">=18.x"
  },
  "keywords": [
    "ES",
    "MODULES",
    "NODE",
    "MODULES",
    "JS"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,jsx,tsx}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.11.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript-eslint": "^8.11.0"
  },
  "dependencies": {
    "@types/node": "^22.7.7",
    "@types/node-fetch": "^2.6.11",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.3.0",
    "jquery": "^3.7.1",
    "jsdom": "^25.0.1",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "workspaces": [
    "packages/scraper-ql-types"
  ]
}